<MudSimpleTable Dense="true" Class="ml-1" Style="overflow-x: auto;" Elevation=0 Square=true>
    <thead>
        <tr>
            @foreach (var h in headings)
            {
                <th>@h</th>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (var row in rows)
        {
            <tr>
                @foreach (var x in row.Split())
                {
                    <td>@x</td>
                }
            </tr>
        }
    </tbody>
</MudSimpleTable>

@code {
    string[] headings = { "Центровка", "Общая глубина, мм", "Глубина конуса, мм" };
    string[] rows = {
